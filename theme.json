					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var:preset|color|mt-green"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)",
